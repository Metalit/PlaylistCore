{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "PlaylistCore",
    "id": "playlistcore",
    "version": "1.4.2",
    "url": "https://github.com/Metalit/PlaylistCore",
    "additionalData": {
      "overrideSoName": "libplaylistcore.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "load": [
        "pwsh ./scripts/restore-backup.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:?"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0"
      ],
      "save": [
        "pwsh ./scripts/get-backup.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    },
    "qmodIncludeDirs": [],
    "qmodIncludeFiles": [],
    "qmodOutput": null
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.6",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3700.0.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.26",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.2",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "rapidjson-macros",
      "versionRange": "^1.2.1",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^1.4.2",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.0.1",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.6.1",
      "additionalData": {}
    },
    {
      "id": "web-utils",
      "versionRange": "^0.6.5",
      "additionalData": {}
    },
    {
      "id": "beatsaverplusplus",
      "versionRange": "^0.1.3",
      "additionalData": {}
    }
  ]
}
